<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/px80"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px79">

        <TextView
            android:id="@+id/tv_contact_search_name"
            style="@style/txt_normal_px22"
            android:layout_width="@dimen/px200"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:maxLines="1"
            android:minWidth="@dimen/px100"
            android:paddingStart="@dimen/px10"
            tools:text="阿华阿华阿华阿华阿华" />

        <ImageView
            android:id="@+id/iv_contact_search_call"
            android:layout_width="@dimen/px45"
            android:layout_height="@dimen/px45"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:src="@drawable/contact_call_selector" />

        <TextView
            android:id="@+id/tv_contact_search_number"
            style="@style/txt_normal_px22"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/px40"
            android:layout_toEndOf="@id/tv_contact_search_name"
            android:layout_toStartOf="@id/iv_contact_search_call"
            android:gravity="start|center_vertical"
            android:maxLines="1"
            tools:text="13500000000" />


    </RelativeLayout>
    <!--  android:background="@drawable/lin_bt_dail" -->
    <!--<View-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="@dimen/px1"-->
    <!--android:background="@color/lin_bt_dail" />-->

</LinearLayout>