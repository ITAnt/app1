<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:background="@drawable/dialog_contact_synchronous_bg"
    android:orientation="vertical">

    <EditText
        android:id="@+id/edit_set_name"
        style="@style/txt_normal_px22"
        android:layout_width="400px"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginLeft="@dimen/px40"
        android:layout_marginRight="@dimen/px40"
        android:layout_marginTop="@dimen/px50"
        android:background="@mipmap/ip_bt_dail"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:paddingLeft="@dimen/px6"
        android:singleLine="true"
        android:textCursorDrawable="@drawable/edit_cursor_color"
        tools:text="vvvvvvvvvvv" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/px70"
        android:layout_marginTop="@dimen/px70"
        android:gravity="center"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_setting_dialog_yes"
            style="@style/txt_normal_px22"
            android:layout_width="@dimen/px120"
            android:layout_height="@dimen/px70"
            android:layout_marginLeft="@dimen/px60"
            android:layout_marginRight="@dimen/px60"
            android:background="@mipmap/dialog_contact_synchronous_text"
            android:gravity="center"
            android:text="@string/dialog_connect" />


        <TextView
            android:id="@+id/tv_setting_dialog_no"
            style="@style/txt_normal_px22"
            android:layout_width="@dimen/px120"
            android:layout_height="@dimen/px70"
            android:layout_marginRight="@dimen/px60"
            android:background="@mipmap/dialog_contact_synchronous_text"
            android:gravity="center"
            android:text="@string/wifi_text_cancel" />
    </LinearLayout>


</LinearLayout>