<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >

        <shape
            android:shape="oval">
            <size
                android:width="30px"
                android:height="30px"
                />
            <corners android:radius="1100dp"/>
            <solid android:color="@color/lucencys"/>
        </shape>
    </item>
    <item
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        android:top="8dp">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
        </shape>
    </item>

</layer-list>